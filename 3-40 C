clrscr();
printf("Enter the source string:\n");
gets(s);
for(i=0;s[i]!='\0';i++)
t[i]=s[i];
t[i]='\0';
printf("\nSource string s=%s",s);
printf("\nTarget string t=%s",t);
getch();
}





